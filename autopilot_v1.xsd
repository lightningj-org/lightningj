  </xs:complexType>

  <xs:complexType name="ModifyStatusRequestType">
    <xs:sequence>
      <xs:element name="enable" type="xs:boolean"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ModifyStatusResponseType">
    <xs:all/>
  </xs:complexType>

  <xs:complexType name="QueryScoresRequestType">
    <xs:sequence>
      <xs:element name="pubkeys" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="pubkeys" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
